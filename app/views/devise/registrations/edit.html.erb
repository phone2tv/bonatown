<h3><%= t("devise.settings") %></h3>
<div class='row'>
  <div class='col-md-4'>
    <%= render 'form_edit' %>
  </div>
  <div class='col-md-4 col-md-offset-2'>
    <h3><%= t("devise.cancel") %></h3>
    <p>Unhappy? <%= button_to t("devise.cancel"), registration_path(resource_name), :data => { :confirm => "Are you sure?" }, class: 'btn btn-danger', :method => :delete %></p>
    <%= link_to "Back", :back %>
  </div>
</div> <!-- .row -->
