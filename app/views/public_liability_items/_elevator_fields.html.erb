<div class='dynamic-fields'>
  <span class="form-fields">
    <%# f.label :kind, class: 'control-label' %>
    <%= f.text_field :kind, class: 'form-control' %>
  </span>
  <span class="form-fields">
    <%# f.label :quantity, class: 'control-label' %>
    <%= f.text_field :quantity, class: 'form-control' %>
  </span>
  <span class="form-fields">
    <%# f.label :year_of_manufacture, class: 'control-label' %>
    <%= f.text_field :year_of_manufacture, class: 'form-control' %>
  </span>
  <span class="form-fields">
    <%# f.label :using_years, class: 'control-label' %>
    <%= f.text_field :using_years, class: 'form-control' %>
  </span>
  <span class="form-fields">
    <%# f.label :overhaul_interval, class: 'control-label' %>
    <%= f.number_field :overhaul_interval, class: 'form-control' %>
  </span>
  <span class="form-fields">
    <%# f.check_box :_destroy %>
    <%# f.label :_destroy, 'Remove Elevator' %>
    <%# link_to_remove_fields "remove", f %>
    <%# f.hidden_field :_destroy %>
    <%# link_to_function "remove", "remove_fields(this)" %>
    <%= f.hidden_field :_destroy %>
    <%= link_to "remove", '#', class: 'remove_fields' %>
  </span>
</div>
