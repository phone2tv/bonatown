<div class='dynamic-fields'>
  <div class="form-group">
    <%= f.label :name, class: 'control-label' %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%# f.label :kind, class: 'control-label' %>
    <%# f.text_field :kind, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%# f.label :nature, class: 'control-label' %>
    <%# f.text_field :nature, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%# f.label :use, class: 'control-label' %>
    <%# f.text_field :use, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%# f.label :sales_area, class: 'control-label' %>
    <%# f.text_field :sales_area, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%# f.label :sales_volumn, class: 'control-label' %>
    <%# f.number_field :sales_volumn, class: 'form-control' %>
  </div>
