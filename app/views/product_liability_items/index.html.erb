<h1>Listing product_liability_items</h1>

<table>
  <thead>
    <tr>
      <th>Business nature</th>
      <th>Each accident limit</th>
      <th>Bodily injury limit</th>
      <th>Property damage limit</th>
      <th>Aggregate limit</th>
      <th>Started at</th>
      <th>Stopped at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @product_liability_items.each do |product_liability_item| %>
      <tr>
        <td><%= product_liability_item.business_nature %></td>
        <td><%= product_liability_item.each_accident_limit %></td>
        <td><%= product_liability_item.bodily_injury_limit %></td>
        <td><%= product_liability_item.property_damage_limit %></td>
        <td><%= product_liability_item.aggregate_limit %></td>
        <td><%= product_liability_item.started_at %></td>
        <td><%= product_liability_item.stopped_at %></td>
        <td><%= link_to 'Show', product_liability_item %></td>
        <td><%= link_to 'Edit', edit_product_liability_item_path(product_liability_item) %></td>
        <td><%= link_to 'Destroy', product_liability_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product liability item', new_product_liability_item_path %>
