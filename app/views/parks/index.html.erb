<div class="row">
  <div class="user">
    <ol class="breadcrumb">
      <li><a href="#">保险超市网</a></li>
      <li><a href="#">会员中心</a></li>
      <li class="active">会员中心首页</li>
    </ol>
  </div>
</div>

<div class="row">
  <div class='col-md-2'>
    <%= render partial: 'users/nav_panels', locals: { user: current_user } %>
  </div>
  <div class='col-md-10'>
    <table class="table table-bordered table-hover">
      <thead>
        <tr>
          <th><%= t('activerecord.attributes.park.name') %></th>
          <th><%= t('activerecord.attributes.park.website') %></th>
          <th><%= t('activerecord.attributes.park.user_id') %></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @parks.each do |park| %>
          <tr>
            <td><%= link_to park.name, park %></td>
            <td><%= park.website %></td>
            <td><%= park.user.profile.name if park.user.present? %></td>
            <td><%= link_to 'Edit', edit_park_path(park) %> / <%= link_to 'Destroy', park, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div> <!-- col-md-10 -->
</div> <!-- row -->
