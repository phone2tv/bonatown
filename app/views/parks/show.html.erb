<div class="row">
  <div class="user">
    <ol class="breadcrumb">
      <li><a href="#">保险超市网</a></li>
      <li><a href="#">会员中心</a></li>
      <li class="active">会员中心首页</li>
    </ol>
  </div>
</div>

<div class="row">
  <div class='col-md-2'>
    <%= render partial: 'shared/mgr_panels', locals: { user: current_user } %>
  </div>
  <div class='col-md-6'>
    <p>
    <strong>Name:</strong>
    <%= @park.name %>
    </p>

    <p>
    <strong>Website:</strong>
    <%= @park.website %>
    </p>

    <p>
    <strong>Aboutme:</strong>
    <%= @park.aboutme %>
    </p>

    <p>
    <strong>User:</strong>
    <%= @park.user.profile.name if @park.user.present? %>
    </p>

    <%= link_to 'Edit', edit_park_path(@park) %> |
    <%= link_to 'Back', parks_path %>
  </div>
</div> <!-- row -->

